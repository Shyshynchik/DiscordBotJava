server:
  servlet:
    context-path: api

springdoc:
  api-docs:
    path: /swagger/config
  swagger-ui:
    path: /swagger
    disable-swagger-default-url: true

spring:
  discord:
    token: 'token'
    create:
      dog:
        active: true
      hello:
        active: true
      delete:
        active: true
      weather:
        active: true
  weather:
    token: 'token'
    default-weather-url: 'https://api.openweathermap.org/data/2.5/weather'